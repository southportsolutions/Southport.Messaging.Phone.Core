<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Authors>Robert Anstett</Authors>
    <Company>Southport Solutions, LLC</Company>
    <Description>Normalized Text Message class to use a fluent API.</Description>
    <Copyright>2023</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/southportsolutions/Southport.Messaging.Phone.Core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/southportsolutions/Southport.Messaging.Phone.Core</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
